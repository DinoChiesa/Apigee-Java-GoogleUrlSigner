<JavaCallout name='Java-URL-Sign-V2'>
  <Properties>
    <Property name='service-account-key'>{private.service_account_key_json}</Property>
    <Property name='verb'>{signedurl_verb}</Property>
    <Property name='bucket'>{request.formparam.bucket}</Property>
    <Property name='object'>{request.formparam.object}</Property>
    <Property name='expires-in'>{signedurl_expiry}</Property>
  </Properties>
  <ClassName>com.google.apigee.callouts.rsa.V2SignedUrlCallout</ClassName>
  <ResourceURL>java://apigee-google-signed-url-20210311.jar</ResourceURL>
</JavaCallout>
