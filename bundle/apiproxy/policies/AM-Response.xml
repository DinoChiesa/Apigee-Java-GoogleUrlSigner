<AssignMessage name='AM-Response'>
  <Set>
    <Payload contentType='application/json'>{
  "status" : "ok",
  "resource" : "{sign_resource}",
  "signature" : "{sign_output}",
  "expiration" : "{sign_expiration}",
  "sample_url" : "https://storage.googleapis.com{sign_resource}?GoogleAccessId=GOOGLE_ACCESS_STORAGE_ID&amp;Expires={sign_expiration}&amp;Signature={sign_output}"
}</Payload>
  </Set>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <AssignTo createNew='false' transport='http' type='response'/>
</AssignMessage>
