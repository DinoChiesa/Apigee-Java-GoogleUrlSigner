<JavaCallout name='Java-URL-Sign-V2-FakeKey'>
  <Properties>
    <Property name='service-account-key'>{my_service_account_key}</Property>
    <Property name='verb'>GET</Property>
    <Property name='resource'>{my_resource}</Property>
    <!-- relative expiry -->
    <Property name='expires-in'>10m</Property>

    <!-- alternatively, you can specify 'expiry' which is a
         quantity of seconds-since-epoch. An 'absolute expiry' time. -->
    <!-- absolute expiry -->
    <!--
     <Property name='expiry'>1597014120</Property>
    -->
  </Properties>
  <ClassName>com.google.apigee.callouts.rsa.V2SignedUrlCallout</ClassName>
  <ResourceURL>java://apigee-google-signed-url-20210311.jar</ResourceURL>
</JavaCallout>
